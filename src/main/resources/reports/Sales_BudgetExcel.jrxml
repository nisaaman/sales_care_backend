	<queryString language="SQL">
		<![CDATA[select lht.*, budget_details.* from 
  
(select lmm.location_id,
(case when target_type = "SALES_OFFICER" then sbd.sales_officer_id 
      when target_type = "DISTRIBUTOR" then disSalesMap.sales_officer_id end) as salesOfficerId, 
au.name as Sales_Officer, concat(prod.name, " ", prod.item_size, " ",uom.abbreviation) Product, ps.pack_size Pack_Size,
dis.distributor_name as Distributor, "zone" Zone_Nmae,
sbd.month Month,
(case when sbd.month = 1 then 'Jan' 
 when sbd.month = 2 then 'Feb' 
 when sbd.month = 3 then 'Mar' 
 when sbd.month = 4 then 'Apr'  
 when sbd.month = 5 then 'May'  
 when sbd.month = 6 then 'Jun' 
 when sbd.month = 7 then 'Jul' 
 when sbd.month = 8 then 'Aug' 
 when sbd.month = 9 then 'Sep' 
 when sbd.month = 10 then 'Oct'
 when sbd.month = 11 then 'Nov'
 when sbd.month = 12 then 'Dec' end) Months,(round((sbd.quantity*sbd.product_trade_price),4)) budget_amount 
from sales_budget sb 
inner join sales_budget_details sbd on sb.id = sbd.sales_budget_id and sb.accounting_year_id = $P{accountingYearId}
and sb.company_id = $P{companyId} and sb.approval_status = "APPROVED" and sb.is_active is true and sb.is_deleted is false and 
(case when target_type = "DISTRIBUTOR" 
             			  then   $X{IN, sbd.distributor_id, distributors}
            when target_type = "SALES_OFFICER"
                        then    $X{IN, sbd.sales_officer_id, salesOfficers}
            else      $X{IN, sbd.location_id, locations}           
end ) 
and  sbd.month >= $P{fromMonthId}  and  sbd.month <=$P{toMonthId}
left join distributor_sales_officer_map disSalesMap on (case when target_type = "DISTRIBUTOR" then sbd.distributor_id end) =
(case when target_type = "DISTRIBUTOR" then disSalesMap.distributor_id end) 
and disSalesMap.is_active is true and disSalesMap.is_deleted is false
left join location loc on (case when target_type = "LOCATION" then sbd.location_id end) = loc.id 
left join reporting_manager rm on 
(case when target_type = "SALES_OFFICER" then sbd.sales_officer_id 
      when target_type = "DISTRIBUTOR" then disSalesMap.sales_officer_id end) 
 = rm.application_user_id and rm.to_date is null and rm.is_active is true 
and rm.is_deleted is false 
left join location_manager_map lmm on rm.reporting_to_id = lmm.application_user_id 
and lmm.to_date is null and lmm.is_active is true and lmm.is_deleted is false 
left join location mloc on lmm.location_id = mloc.id   
left join application_user au on 
(case when target_type = "SALES_OFFICER" then sbd.sales_officer_id 
      when target_type = "DISTRIBUTOR" then disSalesMap.sales_officer_id end) = au.id

inner join product prod on sbd.product_id = prod.id
left join pack_size ps on prod.pack_size_id = ps.id and ps.is_active is true and ps.is_deleted is false
left join unit_of_measurement uom on ps.uom_id = uom.id and uom.is_active is true and uom.is_deleted is false
left join distributor dis on 
(case when target_type = "DISTRIBUTOR" then sbd.distributor_id 
      when target_type = "SALES_OFFICER" then disSalesMap.distributor_id end) = dis.id
         
 order by sbd.month) budget_details 

inner join child_location_hierarchy lht on budget_details.location_id = lht.id;]]>
	</queryString>
