	<queryString>
		<![CDATA[select i.inv_delivery_challan_id
     , i.challan_no
     , i.product_sku
     , i.product_name
     , i.item_size
     , i.pack_size
     , i.base_unit
     , i.trade_price
     , sum(i.price)                                                                         price
     , sum(i.product_quantity)                                                              product_quantity
     , sum(i.discount_value)                                                                discount_value
     , @vat_amount := sum(i.vat_amount)                                                     vat_amount
     , i.vat_rate
     , i.vat_included
     , case when i.vat_included then "-" else CONCAT("@", i.vat_rate, " | ", @vat_amount) end vat_rate_amount
     , sum(i.invoice_amount)                                                                invoice_amount
from (select c.id                                                       inv_delivery_challan_id
           , c.challan_no                                               challan_no
           , p.product_sku                                              product_sku
           , p.name                                                     product_name
           , p.item_size                                                item_size
           , ps.pack_size                                               pack_size
           , uom.abbreviation                                           base_unit
           , ptp.trade_price                                            trade_price
           , @price := ROUND(ptp.trade_price * itd.quantity, 2)         price
           , itd.quantity                                               product_quantity
           , @discount_value := round(case
                                          when td.discount_value is null then 0
                                          when td.calculation_type = 'EQUAL' then td.discount_value * itd.quantity
                                          when td.calculation_type = 'PERCENTAGE'
                                              then (@price * td.discount_value) / 100
                                          end, 2)                       discount_value
           , @after_discount_price := @price - @discount_value          after_discount_price
           , @vat_amount := round(case
                                      when v.vat is null then 0
                                      when v.vat_included is true then @after_discount_price -
                                                                       round(@after_discount_price / ((100 + v.vat) / 100), 2)
                                      else @after_discount_price * (v.vat / 100)
                                      end, 2)                           vat_amount
           , v.vat_included                                             vat_included
           , v.vat                                                      vat_rate
           , round(case
                       when v.vat is null or v.vat_included is true then @after_discount_price
                       else @after_discount_price + @vat_amount end, 2) invoice_amount
      from sales_invoice si
               inner join sales_invoice_challan_map m
                          on m.sales_invoice_id = si.id
                              and si.id = $P{id}
               inner join inv_delivery_challan c
                          on c.id = m.inv_delivery_challan_id
               inner join inv_transaction it
                          on c.inv_transaction_id = it.id
               inner join inv_transaction_details itd
                          on it.id = itd.inv_transaction_id   and itd.is_active is true    and itd.is_deleted is false
               inner join sales_order_details sod
                          on itd.sales_order_details_id = sod.id
                              and sod.is_active is true
                              and sod.is_deleted is false
               inner join sales_booking_details sbd
                          on sod.sales_booking_details_id = sbd.id
                              and sbd.is_active is true
                              and sbd.is_deleted is false
               inner join product p
                          on sbd.product_id = p.id
               inner join unit_of_measurement uom
                          on p.uom_id = uom.id
               inner join pack_size ps
                          on ps.id = p.pack_size_id
               left join vat_setup v
                         on v.product_id = p.id
                             and si.invoice_date between v.from_date and v.to_date
                             and v.vat_included is false
                             and v.is_active is true
                             and v.is_deleted is false
               inner join product_trade_price ptp
                          on ptp.id = sbd.product_trade_price_id
               left join trade_discount td
                         on td.id = sbd.trade_discount_id
      order by c.id, p.id) as i

group by i.inv_delivery_challan_id
       , i.challan_no
       , i.product_sku
       , i.product_name
       , i.item_size
       , i.pack_size
       , i.base_unit
       , i.trade_price
       , i.vat_rate
       , i.vat_included]]>
	</queryString>
